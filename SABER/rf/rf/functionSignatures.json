{
    "amplifier":
    {
        "inputs":
        [
            {"name":"Name","kind":"namevalue","type":[["char","row"],["string"]],"default":"Amplifier"},
            {"name":"Gain","kind":"namevalue","type":["numeric","scalar"],"default":"0"},
            {"name":"NF","kind":"namevalue","type":["numeric","scalar"],"default":"0"},
            {"name":"OIP3","kind":"namevalue","type":["numeric","scalar"],"default":"Inf"},
            {"name":"Zin","kind":"namevalue","type":["numeric","scalar"],"default":"50"},
            {"name":"Zout","kind":"namevalue","type":["numeric","scalar"],"default":"50"}
        ],
        "outputs":
        [
            {"name":"a","type":["amplifier"]}
        ]
    },
    "modulator":
    {
        "inputs":
        [
            {"name":"Name","kind":"namevalue","type":[["char","row"],["string"]],"default":"Modulator"},
            {"name":"Gain","kind":"namevalue","type":["numeric","scalar"],"default":"0"},
            {"name":"NF","kind":"namevalue","type":["numeric","scalar"],"default":"0"},
            {"name":"OIP3","kind":"namevalue","type":["numeric","scalar"],"default":"Inf"},
            {"name":"Zin","kind":"namevalue","type":["numeric","scalar"],"default":"50"},
            {"name":"Zout","kind":"namevalue","type":["numeric","scalar"],"default":"50"},
            {"name":"LO","kind":"namevalue","type":["numeric","scalar",">0"],"default":"1e9"},
            {"name":"ConverterType","kind":"namevalue","type":["choices={'Up','Down'}"],"default":"Up"}
        ],
        "outputs":
        [
            {"name":"m","type":["modulator"]}
        ]
    },
    "nport":
    {
        "inputs":
        [
                {"name":"FileName","kind":"required","type":[["filepath=*.s2p,*.y2p,*.z2p","*.h2p","*.g2p"],["char","row"],["string"]]},
                {"name":"Name","kind":"positional","type":[["char","row"],["string"]]}
        ],
        "outputs":
        [
            {"name":"n","type":["nport"]}
        ]
    },
    "rfbudget":
    {
        "inputs":
        [
            {"mutuallyExclusiveGroup":
                [
                    [
                        {"name":"Elements","kind":"required","type":[["amplifier"],["modulator"],["rfelement"],["nport"],["rf.internal.rfbudget.Element"]],"default":"[]"},
                        {"name":"InputFrequency","kind":"required","type":[["numeric","scalar"],["numeric","vector"]],"default":"[]"},
                        {"name":"AvailableInputPower","kind":"required","type":["numeric","scalar"],"default":"[]"},
                        {"name":"SignalBandwidth","kind":"required","type":["numeric","scalar"],"default":"[]"},
                        {"name":"AutoUpdate","kind":"positional","type":[["logical","scalar"],["numeric","scalar"]],"default":"true"}
                    ],
                    [
                        {"name":"Elements","kind":"namevalue","type":[["amplifier"],["modulator"],["rfelement"],["nport"],["rf.internal.rfbudget.Element"]],"default":"[]"},
                        {"name":"InputFrequency","kind":"namevalue","type":[["numeric","scalar"],["numeric","vector"]],"default":"[]"},
                        {"name":"AvailableInputPower","kind":"namevalue","type":["numeric","scalar"],"default":"[]"},
                        {"name":"SignalBandwidth","kind":"namevalue","type":["numeric","scalar"],"default":"[]"},
                        {"name":"AutoUpdate","kind":"namevalue","type":[["logical","scalar"],["numeric","scalar"]],"default":"true"}
                    ]
                ]
            }
        ],
        "outputs":
        [
            {"name":"b","type":["rfbudget"]}
        ]
    },
    "rfelement":
    {
        "inputs":
        [
            {"name":"Name","kind":"namevalue","type":[["char","row"],["string"]],"default":"RFelement"},
            {"name":"Gain","kind":"namevalue","type":["numeric","scalar"],"default":"0"},
            {"name":"NF","kind":"namevalue","type":["numeric","scalar"],"default":"0"},
            {"name":"OIP3","kind":"namevalue","type":["numeric","scalar"],"default":"Inf"},
            {"name":"Zin","kind":"namevalue","type":["numeric","scalar"],"default":"50"},
            {"name":"Zout","kind":"namevalue","type":["numeric","scalar"],"default":"50"}
        ],
        "outputs":
        [
            {"name":"r","type":["rfelement"]}
        ]
    }
}